{
  "version": 2,
  "name": "darktrace-network",
  "builds": [
    {
      "src": "bomber/bomber.js",
      "use": "@vercel/node"
    },
    {
      "src": "bomber/bomber.html",
      "use": "@vercel/static"
    },
    {
      "src": "osint/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "osint/index2.html",
      "use": "@vercel/static"
    },
    {
      "src": "osint/login.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api",
      "dest": "/bomber/bomber.js"
    },
    {
      "src": "/bomb",
      "dest": "/bomber/bomber.js"
    },
    {
      "src": "/otp",
      "dest": "/bomber/bomber.js"
    },
    {
      "src": "/bomber",
      "dest": "/bomber/bomber.html"
    },
    {
      "src": "/osint/login",
      "dest": "/osint/login.html"
    },
    {
      "src": "/osint/dashboard",
      "dest": "/osint/index2.html"
    },
    {
      "src": "/osint",
      "dest": "/osint/login.html"
    },
    {
      "src": "/",
      "dest": "/osint/login.html"
    },
    {
      "src": "/(.*)",
      "dest": "/osint/login.html"
    }
  ],
  "env": {
    "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY": "pk_test_c3RpcnJpbmctcmFiYml0LTgwLmNsZXJrLmFjY291bnRzLmRldiQ",
    "CLERK_SECRET_KEY": "your-clerk-secret-key-here"
  }
}
