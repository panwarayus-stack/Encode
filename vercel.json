{
  "version": 2,
  "name": "gaurav-osint-tool",
  "builds": [
    {
      "src": "osint/api/auth/register.js",
      "use": "@vercel/node"
    },
    {
      "src": "osint/api/auth/login.js",
      "use": "@vercel/node"
    },
    {
      "src": "osint/api/auth/verify.js",
      "use": "@vercel/node"
    },
    {
      "src": "bomber/bomber.js",
      "use": "@vercel/node"
    },
    {
      "src": "bomber/bomber.html",
      "use": "@vercel/static"
    },
    {
      "src": "osint/login.html",
      "use": "@vercel/static"
    },
    {
      "src": "osint/register.html",
      "use": "@vercel/static"
    },
    {
      "src": "osint/index2.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/register",
      "dest": "/osint/api/auth/register.js"
    },
    {
      "src": "/api/auth/login",
      "dest": "/osint/api/auth/login.js"
    },
    {
      "src": "/api/auth/verify",
      "dest": "/osint/api/auth/verify.js"
    },
    {
      "src": "/api",
      "dest": "/bomber/bomber.js"
    },
    {
      "src": "/bomb",
      "dest": "/bomber/bomber.js"
    },
    {
      "src": "/otp",
      "dest": "/bomber/bomber.js"
    },
    {
      "src": "/bomber",
      "dest": "/bomber/bomber.html"
    },
    {
      "src": "/dashboard",
      "dest": "/osint/index2.html"
    },
    {
      "src": "/login",
      "dest": "/osint/login.html"
    },
    {
      "src": "/register",
      "dest": "/osint/register.html"
    },
    {
      "src": "/",
      "dest": "/osint/login.html"
    },
    {
      "src": "/(.*)",
      "dest": "/osint/login.html"
    }
  ],
  "env": {
    "MONGODB_URI": "your-mongodb-connection-string",
    "JWT_SECRET": "your-jwt-secret-key-here"
  }
}
